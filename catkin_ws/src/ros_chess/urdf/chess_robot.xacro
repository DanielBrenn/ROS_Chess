<?xml version='1.0'?>

<robot name="chess_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

   <!-- Global reference link -->
  <link name="world"/>
  
  <joint name="fixed_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <!-- Arm base link -->
  <link name="base_link">
    <inertial>
      <mass value="5"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1135" ixy="0.0" ixz="0.0" 
               iyy="0.1135" iyz="0.0"
               izz="0.0225"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
      <material name="grey"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="base_link">
    <material>Gazebo/Grey</material>
  </gazebo>

  <!-- Shoulder pan joint -->
  <joint name="1shoulder_pan_joint" type="revolute">
    <limit lower="-3.14" upper="3.14" effort="100.0" velocity="3.14"/>
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  
  <!-- Shoulder link -->
  <link name="shoulder_link">
    <inertial>
      <mass value="2"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0317" ixy="0.0" ixz="0.0"
               iyy="0.0317" iyz="0.0"
               izz="0.0625"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.25" length="0.05"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <cylinder radius="0.25" length="0.05"/>
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="shoulder_link">
    <material>Gazebo/Orange</material>
  </gazebo>






  <!-- Shoulder lift joint -->
  <joint name="2shoulder_lift_joint" type="revolute">
    <limit lower="-1.5708" upper="1.5708" effort="100" velocity="3.14"/>
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.0 0.025" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>


   <!-- Upper arm link -->
  <link name="upper_arm_link">
    <inertial>
      <mass value="3"/>
      <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.2575" ixy="0.0" ixz="0.0"
               iyy="0.2575" iyz="0.0"
               izz="0.0150"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="1"/>
      </geometry>
      <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <cylinder radius="0.1" length="1"/>
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="upper_arm_link">
    <material>Gazebo/Orange</material>
  </gazebo>







  <!-- Elbow joint -->
  <joint name="3elbow_joint" type="revolute">
    <limit lower="-2.3562" upper="2.3562" effort="100" velocity="3.14"/>
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.0 1" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- Forearm link -->
  <link name="forearm_link">
    <inertial>
      <mass value="2.5"/>
      <origin xyz="0.0 0.0 0.125" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.2146" ixy="0.0" ixz="0.0"
               iyy="0.2146" iyz="0.0"
               izz="0.0125"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.10" length="1"/>
      </geometry>
      <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <cylinder radius="0.10" length="1"/>
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="forearm_link">
    <material>Gazebo/Orange</material>
  </gazebo>






  <!-- Wrist joint -->
  <joint name="4wrist_joint" type="revolute">
    <limit lower="-2.3562" upper="2.3562" effort="100" velocity="3.14"/>
    <parent link="forearm_link"/>
    <child link="wrist_link"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.0 1" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>


   <!-- Wrist link -->
  <link name="wrist_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.00009" ixy="0.0" ixz="0.0"
               iyy="0.00009" iyz="0.0"
               izz="0.00002"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="wrist_link">
    <material>Gazebo/Orange</material>
  </gazebo>










  <!-- Gripper base pan joint -->
  <joint name="5gripper_pan_joint" type="revolute">
    <limit lower="-3.14" upper="3.14" effort="100" velocity="3.14"/>
    <parent link="wrist_link"/>
    <child link="gripper_base_link"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  
  <!-- Gripper base -->
  <link name="gripper_base_link">
    <inertial>
      <mass value="2"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0317" ixy="0.0" ixz="0.0"
               iyy="0.0317" iyz="0.0"
               izz="0.0625"
      />
    </inertial>
    <collision>
      <geometry>
        <box size=".1 .3 .02"/>
      </geometry>
      <origin xyz="0.0 0.0 0.01" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <box size=".1 .3 .02"/>
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.01" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="shoulder_link">
    <material>Gazebo/Orange</material>
  </gazebo>









   <!-- Left finger joint -->
  <joint name="6left_finger_joint" type="prismatic">
    <limit lower="0" upper="0.13" effort="100.0" velocity="4.0"/>
    <parent link="gripper_base_link"/>
    <child link="left_finger"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.01 0.1" />
  </joint>

  <!-- Left finger link -->
  <link name="left_finger">
    <inertial>
      <mass value="0.3"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0009" ixy="0.0" ixz="0.0"
               iyy="0.0009" iyz="0.0"
               izz="0.0002"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.1 0.01 0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <box size="0.1 0.01 0.2"/>
      </geometry>
      <material name="blue"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="left_finger">
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <mu1>15</mu1>
    <mu2>15</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>1.0</maxVel>
    <minDepth>0.002</minDepth>
    <material>Gazebo/Blue</material>
  </gazebo>

  <!-- Right finger joint -->
  <joint name="7right_finger_joint" type="prismatic">
    <limit lower="-0.13" upper="0" effort="100.0" velocity="4.0"/>
    <parent link="gripper_base_link"/>
    <child link="right_finger"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 -0.01 0.1" />
  </joint>

  <!-- Right finger link -->
  <link name="right_finger">
    <inertial>
      <mass value="0.3"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0009" ixy="0.0" ixz="0.0"
               iyy="0.0009" iyz="0.0"
               izz="0.0002"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.1 0.01 0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <box size="0.1 0.01 0.2"/>
      </geometry>
      <material name="blue"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="right_finger">
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <mu1>15</mu1>
    <mu2>15</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>1.0</maxVel>
    <minDepth>0.002</minDepth>
    <material>Gazebo/Blue</material>
  </gazebo>





  </robot>


  

