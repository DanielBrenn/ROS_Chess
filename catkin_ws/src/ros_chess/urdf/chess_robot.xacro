<?xml version='1.0'?>

<robot name="chess_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- RViz colors -->
  <xacro:include filename="$(find ros_chess)/urdf/materials.xacro" /> 
  <!-- Transmisions -->
  <xacro:include filename="$(find ros_chess)/urdf/transmission.xacro" />
  <!-- Gazebo plugin -->
  <xacro:include filename="$(find ros_chess)/urdf/chess_robot.gazebo" />

   <!-- Global reference link -->
  <link name="world"/>
  
  <joint name="fixed_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <!-- Arm base link -->
  <link name="base_link">
    <inertial>
      <mass value="5"/>
      <origin xyz="0.0 0.0 0"/>
      <inertia ixx="0.1135" ixy="0.0" ixz="0.0" 
               iyy="0.1135" iyz="0.0"
               izz="0.0225"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
      <material name="grey"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="base_link">
    <material>Gazebo/Grey</material>
  </gazebo>

  <!-- Shoulder pan joint -->
  <joint name="J1_shoulder_pan_joint" type="revolute">
    <limit lower="-3.14" upper="3.14" effort="1000" velocity="3.14"/>
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  
  <!-- Shoulder link -->
  <link name="shoulder_link">
    <inertial>
      <mass value="2"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0317" ixy="0.0" ixz="0.0"
               iyy="0.0317" iyz="0.0"
               izz="0.0625"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.25" length="0.05"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh filename = "package://ros_chess/gazebo_models/robot_arm/shoulder.dae"/>
        <!--
        <cylinder radius="0.25" length="0.05"/>
        -->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="shoulder_link">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- Shoulder lift joint -->
  <joint name="J2_shoulder_lift_joint" type="revolute">
    <limit lower="-1.5708" upper="1.5708" effort="500" velocity="3.14"/>
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.0 0.025" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

   <!-- Upper arm link -->
  <link name="upper_arm_link">
    <inertial>
      <mass value="3"/>
      <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.2575" ixy="0.0" ixz="0.0"
               iyy="0.2575" iyz="0.0"
               izz="0.0150"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="1.5"/>
      </geometry>
      <origin xyz="0.0 0.0 0.75" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh filename = "package://ros_chess/gazebo_models/robot_arm/upper_arm.dae"/>
        <!--
        <cylinder radius="0.1" length="1.5"/>
        -->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="upper_arm_link">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- Elbow joint -->
  <joint name="J3_elbow_joint" type="revolute">
    <limit lower="-2.3562" upper="2.3562" effort="500" velocity="3.14"/>
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.0 1.5" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- Forearm link -->
  <link name="forearm_link">
    <inertial>
      <mass value="2.5"/>
      <origin xyz="0.0 0.0 0.125" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.2146" ixy="0.0" ixz="0.0"
               iyy="0.2146" iyz="0.0"
               izz="0.0125"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.10" length="1.5"/>
      </geometry>
      <origin xyz="0.0 0.0 0.75" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh filename = "package://ros_chess/gazebo_models/robot_arm/forearm.dae"/>
        <!--
        <cylinder radius="0.10" length="1.5"/>
        -->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="1.57079632679  0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="forearm_link">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- Wrist joint -->
  <joint name="J4_wrist_joint" type="revolute">
    <limit lower="-2.3562" upper="2.3562" effort="200" velocity="3.14"/>
    <parent link="forearm_link"/>
    <child link="wrist_link"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.0 1.5" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>


   <!-- Wrist link -->
  <link name="wrist_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.00009" ixy="0.0" ixz="0.0"
               iyy="0.00009" iyz="0.0"
               izz="0.00002"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh filename = "package://ros_chess/gazebo_models/robot_arm/wrist.dae"/>
        <!--<cylinder radius="0.05" length="0.2"/>-->
      </geometry>
      <material name="orange"/>
      <origin xyz="-0.1 -0.15 0.15" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="wrist_link">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- Gripper base pan joint -->
  <joint name="J5_gripper_pan_joint" type="revolute">
    <limit lower="-3.14" upper="3.14" effort="100" velocity="3.14"/>
    <parent link="wrist_link"/>
    <child link="gripper_base_link"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  
  <!-- Gripper base -->
  <link name="gripper_base_link">
    <inertial>
      <mass value="0.03"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0317" ixy="0.0" ixz="0.0"
               iyy="0.0317" iyz="0.0"
               izz="0.0625"
      />
    </inertial>
    <collision>
      <geometry>
        <box size=".1 .3 .02"/>
      </geometry>
      <origin xyz="0.0 0.0 0.01" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh filename = "package://ros_chess/gazebo_models/robot_arm/gripper_base.dae"/>
        <!--
        <box size=".1 .3 .02"/>
        -->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="shoulder_link">
    <material>Gazebo/Orange</material>
  </gazebo>

   <!-- Left finger joint -->
  <joint name="J6_left_finger_joint" type="prismatic">
    <limit lower="0" upper="0.05" effort="50.0" velocity="4.0"/>
    <parent link="gripper_base_link"/>
    <child link="left_finger"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.01 0.1" />
  </joint>

  <!-- Left finger link -->
  <link name="left_finger">
    <inertial>
      <mass value="0.01"/>
      <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0009" ixy="0.0" ixz="0.0"
               iyy="0.0009" iyz="0.0"
               izz="0.0002"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.0375 0.01 0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <box size="0.0375 0.01 0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="left_finger">
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <mu1>15</mu1>
    <mu2>15</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>1.0</maxVel>
    <minDepth>0.002</minDepth>
    <material>Gazebo/Blue</material>
  </gazebo>

  <!-- Right finger joint -->
  <joint name="J7_right_finger_joint" type="prismatic">
    <limit lower="0" upper="0.05" effort="50.0" velocity="4.0"/>
    <parent link="gripper_base_link"/>
    <child link="right_finger"/>
    <axis xyz="0 -1 0"/>
    <origin xyz="0.0 -0.01 0.1" />
  </joint>

  <!-- Right finger link -->
  <link name="right_finger">
    <inertial>
      <mass value="0.01"/>
      <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0009" ixy="0.0" ixz="0.0"
               iyy="0.0009" iyz="0.0"
               izz="0.0002"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.0375 0.01 0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <box size="0.0375 0.01 0.2"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="right_finger">
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <mu1>15</mu1>
    <mu2>15</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>1.0</maxVel>
    <minDepth>0.002</minDepth>
    <material>Gazebo/Blue</material>
  </gazebo>

  <!--End effector (TCP) joint
  <joint name="end_effector_joint" type="fixed">
    <origin xyz="0.0 0.0 0.2" rpy="0 0 0"/>
    <parent link="gripper_base_link"/>
    <child link="end_effector_link"/>
  </joint>

   End effector link 
  <link name="end_effector_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01" />
      </geometry>
      <material name="red"/>
     </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1.0e-03" />
      <inertia ixx="1.0e-03" ixy="0.0" ixz="0.0"
               iyy="1.0e-03" iyz="0.0"
               izz="1.0e-03" />
    </inertial>
  </link>
  
  <gazebo reference="end_effector_link">
    <material>Gazebo/Red</material>
  </gazebo>
  -->
  
   <!-- Camera -->
  <joint type="fixed" name="camera_joint">
    <origin xyz="0 0 0" rpy="0 -1.57 0"/>
    <child link="camera_link"/>
    <parent link="gripper_base_link"/>
    <axis xyz="0 1 0" />
  </joint>

  <link name='camera_link'>
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
    </collision>

    <visual name='camera_link_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".04 .04 .045"/>
      </geometry>
    </visual>

  </link>

  <gazebo reference="camera_link">
    <material>Gazebo/Blue</material>
  </gazebo>

  <joint type="fixed" name="camera_optical_joint">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <child link="camera_link_optical"/>
    <parent link="camera_link"/>
  </joint>

  <link name="camera_link_optical">
  </link>


  </robot>


  

